def mkHomAux :
    ‚àÄ n,
      Œ£'(f : P.pt n ‚ü∂ Q.pt n)(f' : P.pt (n + 1) ‚ü∂ Q.pt (n + 1)),
        f' ‚â´ Q.d (n + 1) n = P.d (n + 1) n ‚â´ f
