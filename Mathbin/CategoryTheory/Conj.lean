    where
  toFun f := Î±.inv â‰« f â‰« Î².Hom
  invFun f := Î±.Hom â‰« f â‰« Î².inv
  left_inv f :=
    show Î±.Hom â‰« (Î±.inv â‰« f â‰« Î².Hom) â‰« Î².inv = f by
      rw [category.assoc, category.assoc, Î².hom_inv_id, Î±.hom_inv_id_assoc, category.comp_id]
  right_inv f :=
