theorem Inf_mem_closure {Œ± : Type u} [TopologicalSpace Œ±] [CompleteLinearOrder Œ±] [OrderTopology Œ±] {s : Set Œ±}
    (hs : s.nonempty) : Inf s ‚àà Closure s :=
