@[simp]
theorem set_to_fun_zero_left {hT : DominatedFinMeasAdditive Œº (0 : Set Œ± ‚Üí E ‚ÜíL[‚Ñù] F) C} :
    setToFun Œº 0 hT f = 0 := by
  by_cases hf : integrable f Œº
  ¬∑ rw [set_to_fun_eq hT hf]
    exact L1.set_to_L1_zero_left hT _
  ¬∑ exact set_to_fun_undef hT hf
#align measure_theory.set_to_fun_zero_left MeasureTheory.set_to_fun_zero_left

theorem set_to_fun_zero_left' (hT : DominatedFinMeasAdditive Œº T C)
    (h_zero : ‚àÄ s, MeasurableSet s ‚Üí Œº s < ‚àû ‚Üí T s = 0) : setToFun Œº T hT f = 0 :=
  by
  by_cases hf : integrable f Œº
  ¬∑ rw [set_to_fun_eq hT hf]
    exact L1.set_to_L1_zero_left' hT h_zero _
  ¬∑ exact set_to_fun_undef hT hf
#align measure_theory.set_to_fun_zero_left' MeasureTheory.set_to_fun_zero_left'

theorem set_to_fun_add (hT : DominatedFinMeasAdditive Œº T C) (hf : Integrable f Œº)
    (hg : Integrable g Œº) : setToFun Œº T hT (f + g) = setToFun Œº T hT f + setToFun Œº T hT g := by
  rw [set_to_fun_eq hT (hf.add hg), set_to_fun_eq hT hf, set_to_fun_eq hT hg, integrable.to_L1_add,
    (L1.set_to_L1 hT).map_add]
#align measure_theory.set_to_fun_add MeasureTheory.set_to_fun_add

theorem set_to_fun_finset_sum' (hT : DominatedFinMeasAdditive Œº T C) {Œπ} (s : Finset Œπ)
    {f : Œπ ‚Üí Œ± ‚Üí E} (hf : ‚àÄ i ‚àà s, Integrable (f i) Œº) :
    setToFun Œº T hT (‚àë i in s, f i) = ‚àë i in s, setToFun Œº T hT (f i) :=
  by
  revert hf
  refine' Finset.induction_on s _ _
  ¬∑ intro h
    simp only [set_to_fun_zero, Finset.sum_empty]
  ¬∑ intro i s his ih hf
    simp only [his, Finset.sum_insert, not_false_iff]
    rw [set_to_fun_add hT (hf i (Finset.mem_insert_self i s)) _]
    ¬∑ rw [ih fun i hi => hf i (Finset.mem_insert_of_mem hi)]
    ¬∑ convert integrable_finset_sum s fun i hi => hf i (Finset.mem_insert_of_mem hi)
      ext1 x
      simp
#align measure_theory.set_to_fun_finset_sum' MeasureTheory.set_to_fun_finset_sum'

theorem set_to_fun_finset_sum (hT : DominatedFinMeasAdditive Œº T C) {Œπ} (s : Finset Œπ)
    {f : Œπ ‚Üí Œ± ‚Üí E} (hf : ‚àÄ i ‚àà s, Integrable (f i) Œº) :
    (setToFun Œº T hT fun a => ‚àë i in s, f i a) = ‚àë i in s, setToFun Œº T hT (f i) :=
  by
  convert set_to_fun_finset_sum' hT s hf
  ext1 a
  simp
#align measure_theory.set_to_fun_finset_sum MeasureTheory.set_to_fun_finset_sum

theorem set_to_fun_neg (hT : DominatedFinMeasAdditive Œº T C) (f : Œ± ‚Üí E) :
    setToFun Œº T hT (-f) = -setToFun Œº T hT f :=
  by
  by_cases hf : integrable f Œº
  ¬∑
    rw [set_to_fun_eq hT hf, set_to_fun_eq hT hf.neg, integrable.to_L1_neg,
      (L1.set_to_L1 hT).map_neg]
  ¬∑ rw [set_to_fun_undef hT hf, set_to_fun_undef hT, neg_zero]
    rwa [‚Üê integrable_neg_iff] at hf
#align measure_theory.set_to_fun_neg MeasureTheory.set_to_fun_neg

theorem set_to_fun_sub (hT : DominatedFinMeasAdditive Œº T C) (hf : Integrable f Œº)
    (hg : Integrable g Œº) : setToFun Œº T hT (f - g) = setToFun Œº T hT f - setToFun Œº T hT g := by
  rw [sub_eq_add_neg, sub_eq_add_neg, set_to_fun_add hT hf hg.neg, set_to_fun_neg hT g]
#align measure_theory.set_to_fun_sub MeasureTheory.set_to_fun_sub

theorem set_to_fun_smul [NontriviallyNormedField ùïú] [NormedSpace ùïú E] [NormedSpace ùïú F]
    (hT : DominatedFinMeasAdditive Œº T C) (h_smul : ‚àÄ c : ùïú, ‚àÄ s x, T s (c ‚Ä¢ x) = c ‚Ä¢ T s x) (c : ùïú)
    (f : Œ± ‚Üí E) : setToFun Œº T hT (c ‚Ä¢ f) = c ‚Ä¢ setToFun Œº T hT f :=
  by
  by_cases hf : integrable f Œº
  ¬∑
    rw [set_to_fun_eq hT hf, set_to_fun_eq hT, integrable.to_L1_smul',
      L1.set_to_L1_smul hT h_smul c _]
  ¬∑ by_cases hr : c = 0
    ¬∑ rw [hr]
      simp
    ¬∑ have hf' : ¬¨integrable (c ‚Ä¢ f) Œº := by rwa [integrable_smul_iff hr f]
      rw [set_to_fun_undef hT hf, set_to_fun_undef hT hf', smul_zero]
#align measure_theory.set_to_fun_smul MeasureTheory.set_to_fun_smul

theorem set_to_fun_congr_ae (hT : DominatedFinMeasAdditive Œº T C) (h : f =·µê[Œº] g) :
    setToFun Œº T hT f = setToFun Œº T hT g :=
  by
  by_cases hfi : integrable f Œº
  ¬∑ have hgi : integrable g Œº := hfi.congr h
    rw [set_to_fun_eq hT hfi, set_to_fun_eq hT hgi, (integrable.to_L1_eq_to_L1_iff f g hfi hgi).2 h]
  ¬∑ have hgi : ¬¨integrable g Œº := by
      rw [integrable_congr h] at hfi
      exact hfi
    rw [set_to_fun_undef hT hfi, set_to_fun_undef hT hgi]
#align measure_theory.set_to_fun_congr_ae MeasureTheory.set_to_fun_congr_ae

theorem set_to_fun_measure_zero (hT : DominatedFinMeasAdditive Œº T C) (h : Œº = 0) :
    setToFun Œº T hT f = 0 := by
  have : f =·µê[Œº] 0 := by simp [h]
  rw [set_to_fun_congr_ae hT this, set_to_fun_zero]
#align measure_theory.set_to_fun_measure_zero MeasureTheory.set_to_fun_measure_zero

theorem set_to_fun_measure_zero' (hT : DominatedFinMeasAdditive Œº T C)
    (h : ‚àÄ s, MeasurableSet s ‚Üí Œº s < ‚àû ‚Üí Œº s = 0) : setToFun Œº T hT f = 0 :=
  set_to_fun_zero_left' hT fun s hs hŒºs => hT.eq_zero_of_measure_zero hs (h s hs hŒºs)
#align measure_theory.set_to_fun_measure_zero' MeasureTheory.set_to_fun_measure_zero'

theorem set_to_fun_to_L1 (hT : DominatedFinMeasAdditive Œº T C) (hf : Integrable f Œº) :
    setToFun Œº T hT (hf.toL1 f) = setToFun Œº T hT f :=
