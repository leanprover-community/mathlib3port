section Function

variable [CompleteSpace F] {T T' T'' : Set Œ± ‚Üí E ‚ÜíL[‚Ñù] F} {C C' C'' : ‚Ñù} {f g : Œ± ‚Üí E}

variable (Œº T)

/-- Extend `T : set Œ± ‚Üí E ‚ÜíL[‚Ñù] F` to `(Œ± ‚Üí E) ‚Üí F` (for integrable functions `Œ± ‚Üí E`). We set it to
0 if the function is not integrable. -/
def setToFun (hT : DominatedFinMeasAdditive Œº T C) (f : Œ± ‚Üí E) : F :=
  if hf : Integrable f Œº then L1Cat.setToL1 hT (hf.toL1 f) else 0
#align measure_theory.set_to_fun MeasureTheory.setToFun

variable {Œº T}

theorem set_to_fun_eq (hT : DominatedFinMeasAdditive Œº T C) (hf : Integrable f Œº) :
    setToFun Œº T hT f = L1Cat.setToL1 hT (hf.toL1 f) :=
  dif_pos hf
#align measure_theory.set_to_fun_eq MeasureTheory.set_to_fun_eq

theorem L1Cat.set_to_fun_eq_set_to_L1 (hT : DominatedFinMeasAdditive Œº T C) (f : Œ± ‚Üí‚ÇÅ[Œº] E) :
    setToFun Œº T hT f = L1Cat.setToL1 hT f := by
  rw [set_to_fun_eq hT (L1.integrable_coe_fn f), integrable.to_L1_coe_fn]
#align measure_theory.L1.set_to_fun_eq_set_to_L1 MeasureTheory.L1Cat.set_to_fun_eq_set_to_L1

theorem set_to_fun_undef (hT : DominatedFinMeasAdditive Œº T C) (hf : ¬¨Integrable f Œº) :
    setToFun Œº T hT f = 0 :=
  dif_neg hf
#align measure_theory.set_to_fun_undef MeasureTheory.set_to_fun_undef

theorem set_to_fun_non_ae_strongly_measurable (hT : DominatedFinMeasAdditive Œº T C)
    (hf : ¬¨AeStronglyMeasurable f Œº) : setToFun Œº T hT f = 0 :=
  set_to_fun_undef hT (not_and_of_not_left _ hf)
#align
  measure_theory.set_to_fun_non_ae_strongly_measurable MeasureTheory.set_to_fun_non_ae_strongly_measurable

theorem set_to_fun_congr_left (hT : DominatedFinMeasAdditive Œº T C)
    (hT' : DominatedFinMeasAdditive Œº T' C') (h : T = T') (f : Œ± ‚Üí E) :
    setToFun Œº T hT f = setToFun Œº T' hT' f :=
