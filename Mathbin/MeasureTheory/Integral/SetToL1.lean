  ¬∑ intro f‚ÇÇ g‚ÇÇ hfg hf‚ÇÇ hf_eq
    have hfg' : f‚ÇÇ =·µê[Œº'] g‚ÇÇ := (measure.absolutely_continuous_of_le_smul hŒº'_le).ae_eq hfg
    rw [‚Üê set_to_fun_congr_ae hT hfg, hf_eq, set_to_fun_congr_ae hT' hfg']
