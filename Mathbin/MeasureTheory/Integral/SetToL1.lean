section Order

variable {G' G'' : Type _} [NormedLatticeAddCommGroup G''] [NormedSpace ‚Ñù G''] [CompleteSpace G'']
  [NormedLatticeAddCommGroup G'] [NormedSpace ‚Ñù G']

theorem set_to_L1_mono_left' {T T' : Set Œ± ‚Üí E ‚ÜíL[‚Ñù] G''} {C C' : ‚Ñù}
    (hT : DominatedFinMeasAdditive Œº T C) (hT' : DominatedFinMeasAdditive Œº T' C')
    (hTT' : ‚àÄ s, MeasurableSet s ‚Üí Œº s < ‚àû ‚Üí ‚àÄ x, T s x ‚â§ T' s x) (f : Œ± ‚Üí‚ÇÅ[Œº] E) :
    setToL1 hT f ‚â§ setToL1 hT' f :=
  by
  refine' Lp.induction one_ne_top _ _ _ _ f
  ¬∑ intro c s hs hŒºs
    rw [set_to_L1_simple_func_indicator_const hT hs hŒºs,
      set_to_L1_simple_func_indicator_const hT' hs hŒºs]
    exact hTT' s hs hŒºs c
  ¬∑ intro f g hf hg hfg_disj hf_le hg_le
    rw [(set_to_L1 hT).map_add, (set_to_L1 hT').map_add]
    exact add_le_add hf_le hg_le
  ¬∑ exact is_closed_le (set_to_L1 hT).Continuous (set_to_L1 hT').Continuous
#align measure_theory.L1.set_to_L1_mono_left' MeasureTheory.L1Cat.set_to_L1_mono_left'

theorem set_to_L1_mono_left {T T' : Set Œ± ‚Üí E ‚ÜíL[‚Ñù] G''} {C C' : ‚Ñù}
    (hT : DominatedFinMeasAdditive Œº T C) (hT' : DominatedFinMeasAdditive Œº T' C')
    (hTT' : ‚àÄ s x, T s x ‚â§ T' s x) (f : Œ± ‚Üí‚ÇÅ[Œº] E) : setToL1 hT f ‚â§ setToL1 hT' f :=
  set_to_L1_mono_left' hT hT' (fun s _ _ x => hTT' s x) f
#align measure_theory.L1.set_to_L1_mono_left MeasureTheory.L1Cat.set_to_L1_mono_left

theorem set_to_L1_nonneg {T : Set Œ± ‚Üí G' ‚ÜíL[‚Ñù] G''} {C : ‚Ñù} (hT : DominatedFinMeasAdditive Œº T C)
    (hT_nonneg : ‚àÄ s, MeasurableSet s ‚Üí Œº s < ‚àû ‚Üí ‚àÄ x, 0 ‚â§ x ‚Üí 0 ‚â§ T s x) {f : Œ± ‚Üí‚ÇÅ[Œº] G'}
    (hf : 0 ‚â§ f) : 0 ‚â§ setToL1 hT f :=
  by
  suffices : ‚àÄ f : { g : Œ± ‚Üí‚ÇÅ[Œº] G' // 0 ‚â§ g }, 0 ‚â§ set_to_L1 hT f
  exact this (‚ü®f, hf‚ü© : { g : Œ± ‚Üí‚ÇÅ[Œº] G' // 0 ‚â§ g })
  refine' fun g =>
    @is_closed_property { g : Œ± ‚Üí‚ÇÅ‚Çõ[Œº] G' // 0 ‚â§ g } { g : Œ± ‚Üí‚ÇÅ[Œº] G' // 0 ‚â§ g } _ _ _
      (dense_range_coe_simple_func_nonneg_to_Lp_nonneg 1 Œº G' one_ne_top) _ _ g
  ¬∑ exact is_closed_le continuous_zero ((set_to_L1 hT).Continuous.comp continuous_induced_dom)
  ¬∑ intro g
    have : (coe_simple_func_nonneg_to_Lp_nonneg 1 Œº G' g : Œ± ‚Üí‚ÇÅ[Œº] G') = (g : Œ± ‚Üí‚ÇÅ‚Çõ[Œº] G') := rfl
    rw [this, set_to_L1_eq_set_to_L1s_clm]
    exact set_to_L1s_nonneg (fun s => hT.eq_zero_of_measure_zero) hT.1 hT_nonneg g.2
#align measure_theory.L1.set_to_L1_nonneg MeasureTheory.L1Cat.set_to_L1_nonneg

theorem set_to_L1_mono {T : Set Œ± ‚Üí G' ‚ÜíL[‚Ñù] G''} {C : ‚Ñù} (hT : DominatedFinMeasAdditive Œº T C)
    (hT_nonneg : ‚àÄ s, MeasurableSet s ‚Üí Œº s < ‚àû ‚Üí ‚àÄ x, 0 ‚â§ x ‚Üí 0 ‚â§ T s x) {f g : Œ± ‚Üí‚ÇÅ[Œº] G'}
    (hfg : f ‚â§ g) : setToL1 hT f ‚â§ setToL1 hT g :=
  by
  rw [‚Üê sub_nonneg] at hfg‚ä¢
  rw [‚Üê (set_to_L1 hT).map_sub]
  exact set_to_L1_nonneg hT hT_nonneg hfg
#align measure_theory.L1.set_to_L1_mono MeasureTheory.L1Cat.set_to_L1_mono

end Order

theorem norm_set_to_L1_le_norm_set_to_L1s_clm (hT : DominatedFinMeasAdditive Œº T C) :
    ‚ÄñsetToL1 hT‚Äñ ‚â§ ‚ÄñsetToL1sClm Œ± E Œº hT‚Äñ :=
  calc
    ‚ÄñsetToL1 hT‚Äñ ‚â§ (1 : ‚Ñù‚â•0) * ‚ÄñsetToL1sClm Œ± E Œº hT‚Äñ :=
      by
      refine'
        ContinuousLinearMap.op_norm_extend_le (set_to_L1s_clm Œ± E Œº hT) (coe_to_Lp Œ± E ‚Ñù)
          (simple_func.dense_range one_ne_top) fun x => le_of_eq _
      rw [Nnreal.coe_one, one_mul]
      rfl
    _ = ‚ÄñsetToL1sClm Œ± E Œº hT‚Äñ := by rw [Nnreal.coe_one, one_mul]
    
#align
  measure_theory.L1.norm_set_to_L1_le_norm_set_to_L1s_clm MeasureTheory.L1Cat.norm_set_to_L1_le_norm_set_to_L1s_clm

theorem norm_set_to_L1_le_mul_norm (hT : DominatedFinMeasAdditive Œº T C) (hC : 0 ‚â§ C)
    (f : Œ± ‚Üí‚ÇÅ[Œº] E) : ‚ÄñsetToL1 hT f‚Äñ ‚â§ C * ‚Äñf‚Äñ :=
  calc
    ‚ÄñsetToL1 hT f‚Äñ ‚â§ ‚ÄñsetToL1sClm Œ± E Œº hT‚Äñ * ‚Äñf‚Äñ :=
      ContinuousLinearMap.le_of_op_norm_le _ (norm_set_to_L1_le_norm_set_to_L1s_clm hT) _
    _ ‚â§ C * ‚Äñf‚Äñ := mul_le_mul (norm_set_to_L1s_clm_le hT hC) le_rfl (norm_nonneg _) hC
    
#align measure_theory.L1.norm_set_to_L1_le_mul_norm MeasureTheory.L1Cat.norm_set_to_L1_le_mul_norm

theorem norm_set_to_L1_le_mul_norm' (hT : DominatedFinMeasAdditive Œº T C) (f : Œ± ‚Üí‚ÇÅ[Œº] E) :
    ‚ÄñsetToL1 hT f‚Äñ ‚â§ max C 0 * ‚Äñf‚Äñ :=
  calc
    ‚ÄñsetToL1 hT f‚Äñ ‚â§ ‚ÄñsetToL1sClm Œ± E Œº hT‚Äñ * ‚Äñf‚Äñ :=
      ContinuousLinearMap.le_of_op_norm_le _ (norm_set_to_L1_le_norm_set_to_L1s_clm hT) _
    _ ‚â§ max C 0 * ‚Äñf‚Äñ :=
      mul_le_mul (norm_set_to_L1s_clm_le' hT) le_rfl (norm_nonneg _) (le_max_right _ _)
    
#align measure_theory.L1.norm_set_to_L1_le_mul_norm' MeasureTheory.L1Cat.norm_set_to_L1_le_mul_norm'

theorem norm_set_to_L1_le (hT : DominatedFinMeasAdditive Œº T C) (hC : 0 ‚â§ C) : ‚ÄñsetToL1 hT‚Äñ ‚â§ C :=
  ContinuousLinearMap.op_norm_le_bound _ hC (norm_set_to_L1_le_mul_norm hT hC)
#align measure_theory.L1.norm_set_to_L1_le MeasureTheory.L1Cat.norm_set_to_L1_le

theorem norm_set_to_L1_le' (hT : DominatedFinMeasAdditive Œº T C) : ‚ÄñsetToL1 hT‚Äñ ‚â§ max C 0 :=
  ContinuousLinearMap.op_norm_le_bound _ (le_max_right _ _) (norm_set_to_L1_le_mul_norm' hT)
#align measure_theory.L1.norm_set_to_L1_le' MeasureTheory.L1Cat.norm_set_to_L1_le'

theorem set_to_L1_lipschitz (hT : DominatedFinMeasAdditive Œº T C) :
    LipschitzWith (Real.toNnreal C) (setToL1 hT) :=
