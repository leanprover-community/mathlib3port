    have h_int : ‚àÄ g : Œ± ‚Üí E, ¬¨integrable g Œº ‚Üí ¬¨integrable g Œº' := fun g =>
      mt fun h => h.ofMeasureLeSmul _ hc hŒº_le
    simp_rw [set_to_fun_undef _ hf, set_to_fun_undef _ (h_int f hf)]
#align measure_theory.set_to_fun_congr_measure MeasureTheory.set_to_fun_congr_measure

theorem set_to_fun_congr_measure_of_add_right {Œº' : Measure Œ±}
