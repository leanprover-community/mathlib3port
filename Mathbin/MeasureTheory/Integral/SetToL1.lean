theorem add (hT : FinMeasAdditive Œº T) (hT' : FinMeasAdditive Œº T') : FinMeasAdditive Œº (T + T') :=
  by
  intro s t hs ht hŒºs hŒºt hst
  simp only [hT s t hs ht hŒºs hŒºt hst, hT' s t hs ht hŒºs hŒºt hst, Pi.add_apply]
