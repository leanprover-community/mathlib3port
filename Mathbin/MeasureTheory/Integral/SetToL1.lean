#align
  measure_theory.tendsto_set_to_fun_approx_on_of_measurable_of_range_subset MeasureTheory.tendsto_set_to_fun_approx_on_of_measurable_of_range_subset

/-- Auxiliary lemma for `set_to_fun_congr_measure`: the function sending `f : Œ± ‚Üí‚ÇÅ[Œº] G` to
`f : Œ± ‚Üí‚ÇÅ[Œº'] G` is continuous when `Œº' ‚â§ c' ‚Ä¢ Œº` for `c' ‚â† ‚àû`. -/
theorem continuous_L1_to_L1 {Œº' : Measure Œ±} (c' : ‚Ñù‚â•0‚àû) (hc' : c' ‚â† ‚àû) (hŒº'_le : Œº' ‚â§ c' ‚Ä¢ Œº) :
    Continuous fun f : Œ± ‚Üí‚ÇÅ[Œº] G =>
      (Integrable.ofMeasureLeSmul c' hc' hŒº'_le (L1Cat.integrableCoeFn f)).toL1 f :=
  by
  by_cases hc'0 : c' = 0
  ¬∑ have hŒº'0 : Œº' = 0 := by
      rw [‚Üê measure.nonpos_iff_eq_zero']
      refine' hŒº'_le.trans _
      simp [hc'0]
    have h_im_zero :
      (fun f : Œ± ‚Üí‚ÇÅ[Œº] G =>
          (integrable.of_measure_le_smul c' hc' hŒº'_le (L1.integrable_coe_fn f)).toL1 f) =
        0 :=
      by
      ext1 f
      ext1
      simp_rw [hŒº'0]
      simp only [ae_zero]
    rw [h_im_zero]
    exact continuous_zero
  rw [Metric.continuous_iff]
  intro f Œµ hŒµ_pos
  use Œµ / 2 / c'.to_real
  refine' ‚ü®div_pos (half_pos hŒµ_pos) (to_real_pos hc'0 hc'), _‚ü©
  intro g hfg
  rw [Lp.dist_def] at hfg‚ä¢
  let h_int := fun f' : Œ± ‚Üí‚ÇÅ[Œº] G => (L1.integrable_coe_fn f').ofMeasureLeSmul c' hc' hŒº'_le
  have :
    snorm (integrable.to_L1 g (h_int g) - integrable.to_L1 f (h_int f)) 1 Œº' = snorm (g - f) 1 Œº' :=
    snorm_congr_ae ((integrable.coe_fn_to_L1 _).sub (integrable.coe_fn_to_L1 _))
  rw [this]
  have h_snorm_ne_top : snorm (g - f) 1 Œº ‚â† ‚àû :=
    by
    rw [‚Üê snorm_congr_ae (Lp.coe_fn_sub _ _)]
    exact Lp.snorm_ne_top _
  have h_snorm_ne_top' : snorm (g - f) 1 Œº' ‚â† ‚àû :=
    by
    refine' ((snorm_mono_measure _ hŒº'_le).trans_lt _).Ne
    rw [snorm_smul_measure_of_ne_zero hc'0, smul_eq_mul]
    refine' Ennreal.mul_lt_top _ h_snorm_ne_top
    simp [hc', hc'0]
  calc
    (snorm (g - f) 1 Œº').toReal ‚â§ (c' * snorm (g - f) 1 Œº).toReal :=
      by
      rw [to_real_le_to_real h_snorm_ne_top' (Ennreal.mul_ne_top hc' h_snorm_ne_top)]
      refine' (snorm_mono_measure (‚áëg - ‚áëf) hŒº'_le).trans _
      rw [snorm_smul_measure_of_ne_zero hc'0, smul_eq_mul]
      simp
    _ = c'.to_real * (snorm (‚áëg - ‚áëf) 1 Œº).toReal := to_real_mul
    _ ‚â§ c'.to_real * (Œµ / 2 / c'.to_real) := mul_le_mul le_rfl hfg.le to_real_nonneg to_real_nonneg
    _ = Œµ / 2 := by
      refine' mul_div_cancel' (Œµ / 2) _
      rw [Ne.def, to_real_eq_zero_iff]
      simp [hc', hc'0]
    _ < Œµ := half_lt_self hŒµ_pos
    
#align measure_theory.continuous_L1_to_L1 MeasureTheory.continuous_L1_to_L1

theorem set_to_fun_congr_measure_of_integrable {Œº' : Measure Œ±} (c' : ‚Ñù‚â•0‚àû) (hc' : c' ‚â† ‚àû)
    (hŒº'_le : Œº' ‚â§ c' ‚Ä¢ Œº) (hT : DominatedFinMeasAdditive Œº T C)
    (hT' : DominatedFinMeasAdditive Œº' T C') (f : Œ± ‚Üí E) (hfŒº : Integrable f Œº) :
    setToFun Œº T hT f = setToFun Œº' T hT' f :=
  by
  -- integrability for `Œº` implies integrability for `Œº'`.
  have h_int : ‚àÄ g : Œ± ‚Üí E, integrable g Œº ‚Üí integrable g Œº' := fun g hg =>
    integrable.of_measure_le_smul c' hc' hŒº'_le hg
  -- We use `integrable.induction`
  refine' hfŒº.induction _ _ _ _ _
  ¬∑ intro c s hs hŒºs
    have hŒº's : Œº' s ‚â† ‚àû := by
      refine' ((hŒº'_le s hs).trans_lt _).Ne
      rw [measure.smul_apply, smul_eq_mul]
      exact Ennreal.mul_lt_top hc' hŒºs.ne
    rw [set_to_fun_indicator_const hT hs hŒºs.ne, set_to_fun_indicator_const hT' hs hŒº's]
  ¬∑ intro f‚ÇÇ g‚ÇÇ h_dish hf‚ÇÇ hg‚ÇÇ h_eq_f h_eq_g
    rw [set_to_fun_add hT hf‚ÇÇ hg‚ÇÇ, set_to_fun_add hT' (h_int f‚ÇÇ hf‚ÇÇ) (h_int g‚ÇÇ hg‚ÇÇ), h_eq_f, h_eq_g]
  ¬∑ refine' is_closed_eq (continuous_set_to_fun hT) _
    have :
      (fun f : Œ± ‚Üí‚ÇÅ[Œº] E => set_to_fun Œº' T hT' f) = fun f : Œ± ‚Üí‚ÇÅ[Œº] E =>
        set_to_fun Œº' T hT' ((h_int f (L1.integrable_coe_fn f)).toL1 f) :=
      by
      ext1 f
