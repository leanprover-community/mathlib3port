theorem set_to_fun_indicator_const (hT : DominatedFinMeasAdditive Œº T C) {s : Set Œ±}
    (hs : MeasurableSet s) (hŒºs : Œº s ‚â† ‚àû) (x : E) :
    setToFun Œº T hT (s.indicator fun _ => x) = T s x :=
