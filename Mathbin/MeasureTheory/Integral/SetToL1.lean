    (h_add : ‚àÄ s, MeasurableSet s ‚Üí Œº s < ‚àû ‚Üí T'' s = T s + T' s) (f : Œ± ‚Üí‚ÇÅ[Œº] E) :
    setToL1 hT'' f = setToL1 hT f + setToL1 hT' f :=
  by
  suffices set_to_L1 hT'' = set_to_L1 hT + set_to_L1 hT' by rw [this, ContinuousLinearMap.add_apply]
  refine' ContinuousLinearMap.extend_unique (set_to_L1s_clm Œ± E Œº hT'') _ _ _ _ _
  ext1 f
  simp only [ContinuousLinearMap.add_comp, ContinuousLinearMap.coe_comp', Function.comp_apply,
    ContinuousLinearMap.add_apply]
  suffices set_to_L1 hT f + set_to_L1 hT' f = set_to_L1s_clm Œ± E Œº hT'' f
    by
    rw [‚Üê this]
    congr
  rw [set_to_L1_eq_set_to_L1s_clm, set_to_L1_eq_set_to_L1s_clm,
    set_to_L1s_clm_add_left' hT hT' hT'' h_add]
#align measure_theory.L1.set_to_L1_add_left' MeasureTheory.L1Cat.set_to_L1_add_left'

theorem set_to_L1_smul_left (hT : DominatedFinMeasAdditive Œº T C) (c : ‚Ñù) (f : Œ± ‚Üí‚ÇÅ[Œº] E) :
    setToL1 (hT.smul c) f = c ‚Ä¢ setToL1 hT f :=
  by
  suffices set_to_L1 (hT.smul c) = c ‚Ä¢ set_to_L1 hT by rw [this, ContinuousLinearMap.smul_apply]
  refine' ContinuousLinearMap.extend_unique (set_to_L1s_clm Œ± E Œº (hT.smul c)) _ _ _ _ _
  ext1 f
  simp only [ContinuousLinearMap.coe_comp', Function.comp_apply, ContinuousLinearMap.smul_comp,
    Pi.smul_apply, ContinuousLinearMap.coe_smul']
  suffices c ‚Ä¢ set_to_L1 hT f = set_to_L1s_clm Œ± E Œº (hT.smul c) f
    by
    rw [‚Üê this]
    congr
  rw [set_to_L1_eq_set_to_L1s_clm, set_to_L1s_clm_smul_left c hT]
#align measure_theory.L1.set_to_L1_smul_left MeasureTheory.L1Cat.set_to_L1_smul_left

theorem set_to_L1_smul_left' (hT : DominatedFinMeasAdditive Œº T C)
    (hT' : DominatedFinMeasAdditive Œº T' C') (c : ‚Ñù)
    (h_smul : ‚àÄ s, MeasurableSet s ‚Üí Œº s < ‚àû ‚Üí T' s = c ‚Ä¢ T s) (f : Œ± ‚Üí‚ÇÅ[Œº] E) :
    setToL1 hT' f = c ‚Ä¢ setToL1 hT f :=
