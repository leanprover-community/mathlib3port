theorem set_to_fun_congr_left' (hT : DominatedFinMeasAdditive Œº T C)
    (hT' : DominatedFinMeasAdditive Œº T' C') (h : ‚àÄ s, MeasurableSet s ‚Üí Œº s < ‚àû ‚Üí T s = T' s)
    (f : Œ± ‚Üí E) : setToFun Œº T hT f = setToFun Œº T' hT' f :=
  by
  by_cases hf : integrable f Œº
  ¬∑ simp_rw [set_to_fun_eq _ hf, L1.set_to_L1_congr_left' T T' hT hT' h]
  ¬∑ simp_rw [set_to_fun_undef _ hf]
