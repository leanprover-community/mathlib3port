/-- If `F i ‚Üí f` in `L1`, then `set_to_fun Œº T hT (F i) ‚Üí set_to_fun Œº T hT f`. -/
theorem tendsto_set_to_fun_of_L1 (hT : DominatedFinMeasAdditive Œº T C) {Œπ} (f : Œ± ‚Üí E)
    (hfi : Integrable f Œº) {fs : Œπ ‚Üí Œ± ‚Üí E} {l : Filter Œπ} (hfsi : ‚àÄ·∂† i in l, Integrable (fs i) Œº)
    (hfs : Tendsto (fun i => ‚à´‚Åª x, ‚Äñfs i x - f x‚Äñ‚Çä ‚àÇŒº) l (ùìù 0)) :
    Tendsto (fun i => setToFun Œº T hT (fs i)) l (ùìù <| setToFun Œº T hT f) := by
  classical
    let f_lp := hfi.to_L1 f
    let F_lp i := if hFi : integrable (fs i) Œº then hFi.toL1 (fs i) else 0
    have tendsto_L1 : tendsto F_lp l (ùìù f_lp) :=
      by
      rw [Lp.tendsto_Lp_iff_tendsto_‚Ñíp']
      simp_rw [snorm_one_eq_lintegral_nnnorm, Pi.sub_apply]
      refine' (tendsto_congr' _).mp hfs
      filter_upwards [hfsi] with i hi
      refine' lintegral_congr_ae _
      filter_upwards [hi.coe_fn_to_L1, hfi.coe_fn_to_L1] with x hxi hxf
      simp_rw [F_lp, dif_pos hi, hxi, hxf]
    suffices tendsto (fun i => set_to_fun Œº T hT (F_lp i)) l (ùìù (set_to_fun Œº T hT f))
      by
      refine' (tendsto_congr' _).mp this
      filter_upwards [hfsi] with i hi
      suffices h_ae_eq : F_lp i =·µê[Œº] fs i
      exact set_to_fun_congr_ae hT h_ae_eq
      simp_rw [F_lp, dif_pos hi]
      exact hi.coe_fn_to_L1
    rw [set_to_fun_congr_ae hT hfi.coe_fn_to_L1.symm]
    exact ((continuous_set_to_fun hT).Tendsto f_lp).comp tendsto_L1
#align measure_theory.tendsto_set_to_fun_of_L1 MeasureTheory.tendsto_set_to_fun_of_L1

theorem tendsto_set_to_fun_approx_on_of_measurable (hT : DominatedFinMeasAdditive Œº T C)
    [MeasurableSpace E] [BorelSpace E] {f : Œ± ‚Üí E} {s : Set E} [SeparableSpace s]
    (hfi : Integrable f Œº) (hfm : Measurable f) (hs : ‚àÄ·µê x ‚àÇŒº, f x ‚àà closure s) {y‚ÇÄ : E}
    (h‚ÇÄ : y‚ÇÄ ‚àà s) (h‚ÇÄi : Integrable (fun x => y‚ÇÄ) Œº) :
    Tendsto (fun n => setToFun Œº T hT (SimpleFunc.approxOn f hfm s y‚ÇÄ h‚ÇÄ n)) atTop
      (ùìù <| setToFun Œº T hT f) :=
  tendsto_set_to_fun_of_L1 hT _ hfi
    (eventually_of_forall (SimpleFunc.integrableApproxOn hfm hfi h‚ÇÄ h‚ÇÄi))
    (SimpleFunc.tendsto_approx_on_L1_nnnorm hfm _ hs (hfi.sub h‚ÇÄi).2)
#align
  measure_theory.tendsto_set_to_fun_approx_on_of_measurable MeasureTheory.tendsto_set_to_fun_approx_on_of_measurable

theorem tendsto_set_to_fun_approx_on_of_measurable_of_range_subset
    (hT : DominatedFinMeasAdditive Œº T C) [MeasurableSpace E] [BorelSpace E] {f : Œ± ‚Üí E}
    (fmeas : Measurable f) (hf : Integrable f Œº) (s : Set E) [SeparableSpace s]
