/-- A `fin_meas_additive` set function whose norm on every set is less than the measure of the
set (up to a multiplicative constant). -/
def DominatedFinMeasAdditive {Œ≤} [SeminormedAddCommGroup Œ≤] {m : MeasurableSpace Œ±} (Œº : Measure Œ±)
    (T : Set Œ± ‚Üí Œ≤) (C : ‚Ñù) : Prop :=
  FinMeasAdditive Œº T ‚àß ‚àÄ s, MeasurableSet s ‚Üí Œº s < ‚àû ‚Üí ‚ÄñT s‚Äñ ‚â§ C * (Œº s).toReal
#align measure_theory.dominated_fin_meas_additive MeasureTheory.DominatedFinMeasAdditive

namespace DominatedFinMeasAdditive

variable {Œ≤ : Type _} [SeminormedAddCommGroup Œ≤] {T T' : Set Œ± ‚Üí Œ≤} {C C' : ‚Ñù}

theorem zero {m : MeasurableSpace Œ±} (Œº : Measure Œ±) (hC : 0 ‚â§ C) :
    DominatedFinMeasAdditive Œº (0 : Set Œ± ‚Üí Œ≤) C :=
  by
  refine' ‚ü®fin_meas_additive.zero, fun s hs hŒºs => _‚ü©
  rw [Pi.zero_apply, norm_zero]
  exact mul_nonneg hC to_real_nonneg
#align measure_theory.dominated_fin_meas_additive.zero MeasureTheory.DominatedFinMeasAdditive.zero

theorem eq_zero_of_measure_zero {Œ≤ : Type _} [NormedAddCommGroup Œ≤] {T : Set Œ± ‚Üí Œ≤} {C : ‚Ñù}
    (hT : DominatedFinMeasAdditive Œº T C) {s : Set Œ±} (hs : MeasurableSet s) (hs_zero : Œº s = 0) :
    T s = 0 := by
  refine' norm_eq_zero.mp _
  refine' ((hT.2 s hs (by simp [hs_zero])).trans (le_of_eq _)).antisymm (norm_nonneg _)
  rw [hs_zero, Ennreal.zero_to_real, mul_zero]
#align
  measure_theory.dominated_fin_meas_additive.eq_zero_of_measure_zero MeasureTheory.DominatedFinMeasAdditive.eq_zero_of_measure_zero

theorem eq_zero {Œ≤ : Type _} [NormedAddCommGroup Œ≤] {T : Set Œ± ‚Üí Œ≤} {C : ‚Ñù} {m : MeasurableSpace Œ±}
    (hT : DominatedFinMeasAdditive (0 : Measure Œ±) T C) {s : Set Œ±} (hs : MeasurableSet s) :
    T s = 0 :=
  eq_zero_of_measure_zero hT hs (by simp only [measure.coe_zero, Pi.zero_apply])
#align
  measure_theory.dominated_fin_meas_additive.eq_zero MeasureTheory.DominatedFinMeasAdditive.eq_zero

theorem add (hT : DominatedFinMeasAdditive Œº T C) (hT' : DominatedFinMeasAdditive Œº T' C') :
    DominatedFinMeasAdditive Œº (T + T') (C + C') :=
